          <div class="btn-area">
            <a href="#" class="btn">
              <i class="icon icon-chat-round-2"></i> Share your story</a>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>


<!--CTA Area 2-->

<div class="cta-area-l-20-2 position-relative">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-xl-5 col-lg-6 col-md-8" data-aos="fade-down" data-aos-duration="800" data-aos-once="true">
