{% extends "template.njk" %}

{#
<!-- Page Info Input Here --> #}

{% set PAGE_INFO = {
page_title : "Social App",
page_wrapper_class : ""

} %}
{% import 'components/Header/_header.njk' as Header %}

{% block main %}
<!-- Site Header -->
{{Header.render({
"additional_header_class": "site-header--menu-right landing-20-menu",
"absolute_header" : "true",
container_fluid : false,
sticky_header :true,
menu_wrapper_class : "mx-auto",
hideMegamenu : false,
header_button : "true",
additional_button_classes : "header-btn-l-20 ms-auto d-none d-xs-inline-flex",
header_button_class : "btn get-btn focus-reset",
header_button_text : "Get The App"

})}}


<!-- Hero Area -->

<div class="hero-area-l-20">
  <div class="container">
    <div class="row align-items-center position-relative justify-content-lg-start justify-content-center ">
      <div class="col-xl-5 col-lg-6 col-md-8 order-lg-1 order-1" data-aos="fade-right" data-aos-duration="800" data-aos-once="true">
        <div class="content">
          <h1>Reading book is fun & easier than ever.</h1>
          <p> One glance at a book and you hear the voice of another person, perhaps this dead for 1,000 years. To read
            is to voyage through time.
          </p>
          <div class="btn-area">
            <a href="#" class="btn focus-reset"> <i class="icon icon-apple"></i> Get The App</a>
            <span>Android App will be released soon</span>
          </div>
        </div>
      </div>
      <div class="col-xl-6 offset-xl-1 col-lg-6 col-md-8 order-lg-1 order-0" data-aos="fade-left" data-aos-duration="800"
        data-aos-once="true">
        <div class="hero-content">
          <img src="image/l11/hero-img.svg" alt="" class="w-100">
        </div>
      </div>
    </div>
  </div>
</div>

<!--CTA Area-->

<div class="cta-area-l-20">
  <div class="container">
    <div class="row align-items-center justify-content-lg-between justify-content-center">
      <div class="col-xxl-5 col-xl-5 col-lg-7 col-md-10 col-sm-11" data-aos="fade-right" data-aos-duration="800"
        data-aos-once="true">
        <div class="content d-flex align-items-center">
          <div class="icon-box">
            <img src="image/l11/hand.svg" alt="">
          </div>
          <div class="content-body">
            <p>Talking book, happiness,
              lifestyle and everything that’s realted books.</p>
          </div>
        </div>
      </div>
      <div
        class="col-xl-7 col-lg-5 col-md-7 col-sm-8 text-lg-end d-flex justify-content-lg-end justify-content-center align-items-center"
        data-aos="fade-left" data-aos-duration="800" data-aos-once="true">
        <div class="cta-l-20-1-btn">
          <a href="#" class="btn focus-reset">Join us on Telegram</a>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-lg-12">
        <div class="cta-border-l-20"></div>
      </div>
    </div>
  </div>
</div>


<!--Feature Area -->

<div class="feature-area-l-19">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-xl-6 col-lg-8 col-md-10" data-aos="fade-down" data-aos-duration="800" data-aos-once="true">
        <div class="section-heading-13 text-center">
          <h2>
            Read unlimited books,
            the biggest online library.
          </h2>
          <p>We know it can be really tough to read new books nowadays with our busy life. That’s the reason we’ve made
            booklov for you.</p>
        </div>
      </div>
    </div>
    <div class="row feature-area-l-19-items justify-content-center align-items-center">
      <div class="col-xl-3 col-lg-4 col-sm-6" data-aos="fade-right" data-aos-duration="400" data-aos-once="true">
        <div class="single-features text-center">
          <img src="image/l11/service-icon1.svg" alt="">
          <h5>Largest Collection of Books</h5>
        </div>
      </div>
      <div class="col-xl-3 col-lg-4 col-sm-6" data-aos="fade-right" data-aos-duration="600" data-aos-once="true">
        <div class="single-features text-center">
          <img src="image/l11/service-icon2.svg" alt="">
          <h5>One Click Easy Subscription</h5>
        </div>
      </div>
      <div class="col-xl-3 col-lg-4 col-sm-6" data-aos="fade-right" data-aos-duration="800" data-aos-once="true">
        <div class="single-features text-center">
          <img src="image/l11/service-icon3.svg" alt="">
          <h5>Unlimited Books,
            Anytime you want</h5>
        </div>
      </div>
    </div>
  </div>
</div>


<!--Content Area 1-->

<div class="content-area-l-20-1">
  <div class="container">
    <div class="row align-items-center justify-content-lg-start justify-content-center">
      <div class="col-xl-6 col-lg-6 col-md-8" data-aos="fade-right" data-aos-duration="800"
        data-aos-once="true">
        <div class="image-group-1">
          <div class="main-image">
            <img src="image/l11/content-img1.svg" alt="">
          </div>
          <div class="image-bottom-shape-l-20 d-none d-lg-block"></div>
        </div>
      </div>

      <div class="offset-xxl-1 col-xxl-4 col-xl-5 col-lg-6 col-md-8" data-aos="fade-left"
        data-aos-duration="800" data-aos-once="true">
        <div class="content section-heading-13">
          <h2>
            Over one million <br class="d-sm-block d-none"> books at your hand.
          </h2>
          <p>
            One glance at a book and you hear the voice of another person, perhaps someone dead
            for 1,000 years. To read is to voyage through time.
          </p>
        </div>
      </div>
    </div>
  </div>
</div>


<!--Content Area 2-->

<div class="content-area-l-20-2">
  <div class="container">
    <div class="row align-items-center justify-content-center">
      <div class="col-xxl-5 col-xl-5 col-lg-6 col-md-8 order-lg-1 order-1" data-aos="fade-right" data-aos-duration="800"
        data-aos-once="true">
        <div class="content section-heading-13">
          <h2>
            Switch between your reading modes easily.
          </h2>
          <p>
            One glance at a book and you hear the voice of another person, perhaps someone dead
            for 1,000 years. To read is to voyage through time.
          </p>
        </div>
      </div>
      <div class="offset-xxl-1  col-xl-5 col-lg-6 col-md-8 position-relative order-lg-1 order-0" data-aos="fade-left"
        data-aos-duration="800" data-aos-once="true">
        <div class="image-group-2">
          <div class="main-image">
            <img src="image/l11/content-img2.svg" alt="">
          </div>
          <div class="image-bottom-shape-l-20 d-none d-lg-block"></div>
        </div>
      </div>
    </div>
  </div>
</div>

<!--Content Area 3-->

<div class="content-area-l-20-3">
  <div class="container">
    <div class="row align-items-center justify-content-lg-start  justify-content-center">
      <div class="col-xl-5 col-lg-6 col-md-8 position-relative" data-aos="fade-right" data-aos-duration="800"
        data-aos-once="true">
        <div class="image-group-1">
          <div class="main-image">
            <img src="image/l11/content-img3.svg" alt="">
          </div>
          <div class="image-bottom-shape-l-20 d-none d-lg-block"></div>
        </div>
      </div>

      <div class="offset-xxl-1 col-xxl-4 col-xl-5 col-lg-6 col-md-8" data-aos="fade-left"
        data-aos-duration="800" data-aos-once="true">
        <div class="content section-heading-13">
          <h2>
            One click purchase, <br class="d-sm-block d-none"> instant reading.
